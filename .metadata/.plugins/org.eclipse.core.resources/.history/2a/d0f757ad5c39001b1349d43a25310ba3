
public class Test123 {

	public static void main(String[] args) {
		Test123 t1 = new Test123();
		t1.test2();
	}
	public void test1() {
		int a = 1;
		int b = 2;
		int c;
		
		boolean d;
		
		c = a++ + --b;
		System.out.println("a = " + a + ", b = " + b + ", c = " + c);
		
		d = a-- == b++;
		System.out.println(d == true ? "True!!!" : "False!!!");
		System.out.println("a = " + a + ", b = " + b);
	}
	public void test2() {
        int a = 20;
        int b = a--;
        int c = 40;
        //a=19 b=20 c=40
        
        System.out.println("a = "+ a);
        //a=19
        System.out.println(c + a++ == 59 ? "µþ±â" : "¹Ù³ª³ª");
        //µþ±â, a=20
        System.out.println(c /= b); //2
        //2
        
    }
}
